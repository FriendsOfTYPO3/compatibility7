{
	"name": "friendsoftypo3/compatibility7",
	"type": "typo3-cms-extension",
	"description": "Compatibility Extension for TYPO3 8 LTS",
	"homepage": "https://extensions.typo3.org/extension/compatibility7/",
	"support": {
		"issues": "https://github.com/FriendsOfTYPO3/compatibility7/issues",
		"source": "https://github.com/FriendsOfTYPO3/compatibility7",
		"docs": "https://docs.typo3.org/p/friendsoftypo3/compatibility7/main/en-us/"
	},
	"license": ["GPL-2.0-or-later"],
	"require": {
		"typo3/cms-core": "^8.7.0"
	},
	"replace": {
		"typo3-ter/compatibility7": "*"
	},
	"conflict": {
		"typo3/cms-compatibility6": "*"
	},
	"extra": {
		"typo3/class-alias-loader": {
			"class-alias-maps": [
				"Migrations/Code/ClassAliasMap.php"
			]
		},
		"typo3/cms": {
			"extension-key": "compatibility7"
		}
	},
	"autoload": {
		"psr-4": {
			"TYPO3\\CMS\\Compatibility7\\": "Classes/"
		}
	}
}
