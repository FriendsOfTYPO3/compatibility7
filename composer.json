{
	"name": "friendsoftypo3/compatibility7",
	"type": "typo3-cms-extension",
	"description": "Compatibility Extension for TYPO3 8 LTS",
	"homepage": "https://typo3.org",
	"license": ["GPL-2.0+"],

	"require": {
		"typo3/cms-core": "*"
	},
	"replace": {
		"compatibility7": "*"
	},
	"conflict": {
		"typo3/cms-compatibility6": "*"
	},
	"extra": {
		"typo3/class-alias-loader": {
			"class-alias-maps": [
				"Migrations/Code/ClassAliasMap.php"
			]
		}
	},
	"autoload": {
		"psr-4": {
			"TYPO3\\CMS\\Compatibility7\\": "Classes/"
		}
	}
}
